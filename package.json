{
  "name": "mpd-web-ui",
  "version": "0.0.0-development",
  "description": "Web port of MPD",
  "main": "index.js",
  "scripts": {
    "clean": "npx rimraf build",
    "start": "vite",
    "build": "yarn clean && vite build",
    "compress": "find ./build \\( -name '*.js' -or -name '*.css' \\) -type f -exec gzip -f {} +",
    "serve": "yarn build && yarn compress && npx http-server -g -p 8888 ./build",
    "prepare": "npx husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md,css,less,sass,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexh250786313/mpd-web-ui.git"
  },
  "keywords": [
    "mpd",
    "client",
    "ui",
    "web",
    "frontend"
  ],
  "author": "hexh <250786313@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hexh250786313/mpd-web-ui/issues"
  },
  "homepage": "https://github.com/hexh250786313/mpd-web-ui#readme",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "https-server": "^0.1.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "type-fest": "^2.16.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.14",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "dependencies": {
    "@react-hookz/web": "^15.0.1",
    "eventemitter3": "^4.0.7",
    "immer": "^9.0.15",
    "jotai": "^1.7.4",
    "lodash-es": "^4.17.21",
    "neverthrow": "^4.4.2",
    "ohmyfetch": "^0.4.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-spring": "^9.5.2",
    "swr": "^1.3.0",
    "use-immer": "^0.7.0"
  }
}
